{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build x86_64",
            "type": "cargo",
            "command": "build",
            "args": [
                "--target",
                "x86_64-unknown-none",
                "--profile",
                "dev"
            ],
            "group": "build",
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": true,
                "clear": true
            }
        },
        {
            "label": "QEMU x86_64",
            "type": "process",
            "command": "make",
            "dependsOn": "Build x86_64",
            "isBackground": true,
            "isBuildCommand": false,
            "args": [
                "start-x86_64"
            ],
            "problemMatcher": []
        }
    ]
}